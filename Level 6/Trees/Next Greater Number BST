//O(logn) and O(1)
public class Solution {
	public TreeNode getSuccessor(TreeNode a, int b) {
	    TreeNode root = a;
	    int p = b;
	    if(root==null)
        return null;
 
    TreeNode next = null;
    TreeNode c = root;
    while(c!=null && c.val!=p){
        if(c.val > p){
            next = c;
            c = c.left;
        }else{
            c= c.right;
        }
    }
 
    if(c==null)        
        return null;
 
    if(c.right==null)
        return next;
 
    c = c.right;
    while(c.left!=null)
        c = c.left;
 
    return c;
	}
}
