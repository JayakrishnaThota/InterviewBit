public class Solution {
	public int removeElement(ArrayList<Integer> nums, int val) {
	    if(nums==null || nums.size()==0) return 0;
        int index = 0;
        for(int i=0;i<nums.size();i++){
            if(nums.get(i)==val) continue;
            nums.set(index++,nums.get(i));
        }
        return index;
	}
}
